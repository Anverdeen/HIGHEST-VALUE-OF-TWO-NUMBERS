#include <iostream>

using namespace std;

int main()
{
    int n1,n2,i,s1,s2;
    cin >>n1;
    int a[n1];
    for(i=0;i<n1;i++)
    {
        cin>>a[i];
    }
    cin>>n2;
    int a1[n2];
    for(i=0;i<n2;i++)
    {
        cin>>a1[i];
    }
    s1=0;
    for(i=0;i<n1;i++)
    {
        s1=s1+a[i];
    }
    cout<<s1;
    cout<<"\n";
    s2=0;
    for(i=0;i<n2;i++)
    {
        s2=s2+a1[i];
    }
    cout<<s2;
    cout<<"\n";
    if(s1>s2)
    {
        cout<<s1<<"is the wealth of s1" ;
    }
    else 
    {
        cout<<s2<<"is the wealth of s2";
    }
}
